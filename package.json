{
  "name": "tangerine",
  "version": "0.1.0",
  "description": "Tangerine",
  "private": true,
  "scripts": {
    "build": "nwb build-react-app --no-vendor && npm run build-clean",
    "build-clean": "rm dist/*.map && rm dist/index.html && mv dist/app*.css dist/main.min.css && mv dist/app*.js dist/main.min.js && mv dist/manifest*.js dist/manifest.min.js",
    "clean": "nwb clean-app",
    "start": "nwb serve-react-app --port 5000",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "axios": "^0.17.1",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "nwb-stylus": "^0.7.0",
    "promise.prototype.finally": "^3.1.0",
    "react": "^16.2.0",
    "react-big-calendar": "^0.18.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-select": "^1.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "nwb": "0.21.x"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
